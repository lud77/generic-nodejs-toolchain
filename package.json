{
  "logo": {
    "url": "pkg/logo/logo.jpg",
    "description": "Project logo"
  },
  "name": "basic-tooling",
  "version": "0.0.0",
  "description": "A boilerplate for *Node.js* projects.",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -r docs/**/* && true",
    "lint": "eslint src/**/*.js -c conf/build/eslint.json",
    "test": "mocha test/**/*.js",
    "cover": "istanbul cover _mocha -- -u exports -R spec test/**/*.js",
    "build:docs": "jsdoc src -r -d docs",
    "build:readme": "hbsrender -t pkg/readme/README.md.hbs -c ./package.json -p contribute:pkg/readme/CONTRIBUTE.md.hbs -p installation:pkg/readme/INSTALL.md.hbs > README.md",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "author": "Stefano Butera <stefano.butera@nethnic.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lud77/basic-tooling.git"
  },
  "documentation": "docs/index.html",
  "license": "MIT",
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.8.0",
    "handlebars": "4.0.6",
    "hbsrender": "1.0.0",
    "husky": "0.11.9",
    "istanbul": "0.4.5",
    "jsdoc": "3.4.2",
    "mocha": "3.0.2",
    "onchange": "3.0.2",
    "shx": "0.1.4",
    "sinon": "1.17.5"
  },
  "dependencies": {
    "fs-extra": "0.30.0",
    "immutable": "3.8.1",
    "winston": "2.2.0"
  }
}
